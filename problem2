<php
function compareTriplets($a, $b) {
$puntosA=0;
$puntosB=0;
$i=0;
 $arrayNum= count($a);
 while($i <= $arrayNum) {

     if($a[$i] > $b[$i]){
         $puntosA ++;
     }

     if($a[$i] < $b[$i]){
         $puntosB ++;
     }

    $i++;    
 }
 
$arr = array((string)$puntosA , (string)$puntosB);
return $arr  ;

}
?>
