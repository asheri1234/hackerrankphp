<php

function simpleArraySum($ar) {
            
          $sum = 0;
                    
          for ($i = 0; $i < count($ar); $i++) {
            
                  $sum = $sum + $ar[$i];

}

    return $sum;
    
$ar =array(1, 2, 3, 4, 10, 11); 
$n = sizeof($ar); 
echo "Sum of given array is ",
                array_count_sum($ar); 
  
}

?>
